{
  "name": "acute-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run test-api\" \"ng serve\"",
    "build": "ng build",
    "build:express": "ng build --base-href /app/",
    "test": "ng test",
    "lint": "ng lint",
    "test-api": "json-server --watch api/db.json",
    "e2e": "ng e2e --port=4201",
    "e2e:answers": "ng e2e --port=4201 --suite='answers'",
    "e2e:exercises": "ng e2e --port=4201 --suite='exercises'",
    "e2e:authenticated": "ng e2e --port=4201 --suite='authenticated'",
    "e2e:smoke": "protractor protractor/protractor.conf.js --baseUrl='http://localhost:4200' --grep='#smoke'",
    "ci:start": "node ./bin/www",
    "ci:test": "ng test --watch=false",
    "ci:e2e": "protractor ./protractor/protractor.conf.js --baseUrl='http://localhost:4201/app/' --suite='answer3'",
    "ci:run": "start-server-and-test ci:start http://localhost:4201/app ci:e2e",
    "ci": "run-s lint ci:test build:express ci:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.5",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/pwa": "^0.803.4",
    "@angular/router": "~8.2.5",
    "@angular/service-worker": "~8.2.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "hammerjs": "^2.0.8",
    "jade": "^1.11.0",
    "json-server": "^0.15.1",
    "moment": "^2.24.0",
    "ngx-cookie-service": "^2.2.0",
    "pug": "^2.0.4",
    "rxjs": "~6.4.0",
    "rxjs-compat": "^6.5.3",
    "serve-favicon": "^2.5.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concurrently": "^4.1.2",
    "es6-shim": "^0.35.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.13",
    "start-server-and-test": "^1.10.6",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
